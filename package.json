{
  "name": "spendings-calculator",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "^10"
  },
  "scripts": {
    "start:frontend": "cd frontend && vue-cli-service serve",
    "build:frontend": "cd frontend && vue-cli-service build",
    "build:backend": "cd backend && webpack",
    "lint:frontend": "cd frontend && vue-cli-service lint",
    "start:backend": "cd backend && nodemon --exec babel-node src/index.js --config-file ../babel.config.js",
    "start": "npx concurrently \"yarn start:frontend\" \"yarn start:backend\""
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "backend": "^0.0.0",
    "bcrypt": "^3.0.7",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "d3": "^5.14.2",
    "date-fns": "^2.6.0",
    "frontend": "^2.0.0-alpha.4",
    "jsonwebtoken": "^9.0.0",
    "jwt-decode": "^2.2.0",
    "mongoose": "^5.7.7",
    "node-sass": "^4.12.0",
    "nodemon-webpack-plugin": "^4.1.1",
    "pretty-quick": "^2.0.1",
    "ramda": "^0.26.1",
    "sass-loader": "^8.0.0",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/node": "^7.7.4",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-decorators": "^7.7.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.7.5",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.1",
    "@babel/runtime-corejs3": "^7.7.6",
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "babel-polyfill": "^6.26.0",
    "core-js": "^3.5.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.1.0",
    "nodemon": "^1.19.4",
    "sass-loader": "^8.0.0",
    "vue-loader": "^15.7.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  }
}
