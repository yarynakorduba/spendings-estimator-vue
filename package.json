{
  "name": "spendings-calculator",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "backend",
    "frontend"
  ],
  "scripts": {
    "start:frontend": "cd frontend && vue-cli-service serve",
    "build:frontend": "cd frontend && vue-cli-service build",
    "lint:frontend": "cd frontend && vue-cli-service lint"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "date-fns": "^2.6.0",
    "frontend": "^2.0.0-alpha.4",
    "node-sass": "^4.12.0",
    "ramda": "^0.26.1",
    "sass-loader": "^8.0.0"
  }
}
